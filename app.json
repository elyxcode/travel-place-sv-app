{
  "expo": {
    "name": "SLV Travel",
    "slug": "slv-travel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher.png",
    "scheme": "travelplacesvapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elyxdev.travelplacesvapp"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.elyxdev.travelplacesvapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ic_launcher.png",
          "backgroundColor": "#0085ff",
          "imageWidth": 200
        }
      ],
      "expo-font",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3887333312020428~1246664603",
          "iosAppId": "ca-app-pub-3887333312020428~1840974241"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}